            </template>
          </element>

          <element type="text/plain">
            <style>
              <compound-key name="absolute_pos">
                <basic-object name="x">-50.0</basic-object>
                <basic-object name="y">5.0</basic-object>
              </compound-key>
              <compound-key name="min-size">
