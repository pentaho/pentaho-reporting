<?xml version="1.0" encoding="ISO-8859-1"?>
<report
    xmlns="http://jfreereport.sourceforge.net/namespaces/reports/legacy/simple"
    name="Page-Counting" pageformat="A4" topmargin="25" leftmargin="25" bottommargin="25" rightmargin="25">
  <configuration>
    <property
        name="org.pentaho.reporting.engine.classic.core.modules.output.pageable.pdf.Encoding">Identity-H</property>
    <property name="org.pentaho.reporting.engine.classic.core.modules.output.pageable.pdf.EmbedFonts">true</property>
  </configuration>

  <items>
    <string-field name="Page-Field" dynamic="true" width="25%" x="0" fieldname="page"/>
  </items>

  <functions>
    <expression name="page" class="org.pentaho.reporting.engine.classic.core.function.PageFunction"/>
  </functions>
</report>
