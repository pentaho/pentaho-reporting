<?xml version="1.0" encoding="ISO-8859-1"?>
<report
    xmlns="http://jfreereport.sourceforge.net/namespaces/reports/legacy/simple"
    name="Nested Rows" pageformat="A4" topmargin="25" leftmargin="25" bottommargin="25" rightmargin="25">

  <reportheader name="ReportHeader" layout="block">
    <band layout="block">
      <band name="test-b1" layout="block">
        <label width="50">Test</label>
      </band>
      <band name="test-b2" layout="block" padding-top="2" padding-bottom="2">
        <label width="50">Test</label>
      </band>
      <band name="test-b3" layout="block">
        <label width="50">Test</label>
      </band>
    </band>

    <band layout="block">
      <band name="test1" layout="row">
        <label width="50">Test</label>
        <label width="50">Test</label>
        <label width="50">Test</label>
      </band>
      <band name="test2" layout="row" padding-top="2" padding-bottom="2">
        <label width="50">Test</label>
        <label width="50">Test</label>
        <label width="50">Test</label>
      </band>
      <band name="test3" layout="row">
        <label width="50">Test</label>
        <label width="50">Test</label>
        <label width="50">Test</label>
      </band>
    </band>
  </reportheader>
</report>
