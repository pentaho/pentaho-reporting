<?xml version="1.0" encoding="ISO-8859-1"?>
<report
    xmlns="http://jfreereport.sourceforge.net/namespaces/reports/legacy/simple"
    name="SportsCouncil" pageformat="A4" topmargin="25" leftmargin="25" bottommargin="25" rightmargin="25">
  <configuration>
    <property
        name="org.pentaho.reporting.engine.classic.core.modules.output.pageable.pdf.Encoding">Identity-H</property>
    <property name="org.pentaho.reporting.engine.classic.core.modules.output.pageable.pdf.EmbedFonts">true</property>
  </configuration>

  <reportheader name="ReportHeader" fontsize="30" layout="block">
    <band height="100%">
      <label name="HEADER-LABEL-01" x="100" y="10" width="300" height="70" alignment="center"
             vertical-alignment="middle">HEADER LABEL
      </label>
    </band>
    <label name="HEADER-LABEL-01" x="100" y="10" width="300" height="70" alignment="center" vertical-alignment="middle">
      HEADER LABEL
    </label>
    <band layout="canvas">
      <band layout="canvas">
        <label name="HEADER-LABEL-01" x="100" y="110%" width="300" height="70" alignment="center"
               vertical-alignment="middle">HEADER LABEL
        </label>
      </band>
      <label name="HEADER-LABEL-01" x="100" y="110%" width="300" height="70" alignment="center"
             vertical-alignment="middle">HEADER LABEL
      </label>
    </band>
    <band layout="inline">
      <label name="HEADER-LABEL-01" x="100" y="110%" width="300" height="70" alignment="center"
             vertical-alignment="middle">HEADER LABEL
      </label>
    </band>

  </reportheader>

  <pageheader>
    <label name="HEADER-LABEL-01" x="100" y="10" width="300" height="70" alignment="center" vertical-alignment="middle">
      HEADER LABEL
    </label>
  </pageheader>

  <pagefooter>
    <label name="FOOTER-LABEL-01" x="100" y="10" width="300" height="70" alignment="center" vertical-alignment="middle">
      FOOTER LABEL
    </label>
  </pagefooter>
</report>
