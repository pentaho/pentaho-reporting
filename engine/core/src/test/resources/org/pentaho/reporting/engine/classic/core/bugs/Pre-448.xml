          border-left-color="black" border-left-style="solid" border-left-width="1"
          border-right-color="black" border-right-style="solid" border-right-width="1"
          name="reportheader" layout="block">
      <band name="test-01" height="70" width="555" layout="block">
        <label width="555" height="70" padding-top="10" padding-left="10" padding-right="10"
               name="rect">Test
